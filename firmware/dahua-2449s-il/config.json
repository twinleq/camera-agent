{
  "camera": {
    "model": "dahua-2449s-il",
    "platform": "linux_arm",
    "architecture": "armv7",
    "ip": "127.0.0.1",
    "rtsp_port": 554,
    "username": "admin",
    "password": "admin",
    "auto_detect_ip": true,
    "stream_paths": [
      "/cam/realmonitor?channel=1&subtype=0",
      "/cam/realmonitor?channel=1&subtype=1"
    ]
  },
  "agent": {
    "agent_id": "dahua-2449s-il-001",
    "device_id": "",
    "connection_mode": "hybrid",
    "log_level": "INFO",
    "ssl_cert_file": "/etc/camera_agent/agent.pem",
    "ssl_root_cert": "/etc/camera_agent/root.crt",
    "config_path": "/mnt/mtd/Config/camera_agent.cfg"
  },
  "tunnel": {
    "enabled": true,
    "server_url": "ws://your-tunnel-server.com:8080/tunnel",
    "server_token": "your-secret-token",
    "tunnel_port": 2956,
    "connection_timeout": 30,
    "reconnect_interval": 10,
    "heartbeat_interval": 30
  },
  "p2p": {
    "enabled": true,
    "stun_servers": [
      "stun:stun.l.google.com:19302",
      "stun:stun1.l.google.com:19302"
    ],
    "turn_servers": [
      {
        "url": "turn:turn-server.com:3478",
        "username": "user",
        "password": "pass"
      }
    ],
    "registry_url": "https://p2p-registry.com/api",
    "ice_timeout": 30,
    "fallback_to_tunnel": true
  },
  "streaming": {
    "quality": "high",
    "buffer_size": 2097152
  }
}